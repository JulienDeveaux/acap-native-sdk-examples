{
    "schemaVersion": "1.8.0",
    "acapPackageConf": {
        "setup": {
            "friendlyName": "Flatten Image",
            "appName": "flatten_image",
            "vendor": "Axis Communications",
            "embeddedSdkVersion": "3.0",
            "vendorUrl": "https://www.axis.com",
            "runMode": "respawn",
            "version": "1.0.0"
        },
        "configuration": {
            "httpConfig": [
                {
                    "access": "viewer",
                    "name": "image.cgi",
                    "type": "fastCgi"
                }
            ],
            "paramConfig": [
                {
                    "name": "LensType",
                    "default": "fisheye",
                    "type": "enum:fisheye,dual_fisheye,panoramic"
                },
                {
                    "name": "Projection",
                    "default": "fisheye_undistort",
                    "type": "enum:fisheye_undistort,equirectangular,rectilinear,cylindrical"
                },
                {
                    "name": "InputFOV",
                    "default": "180",
                    "type": "string"
                },
                {
                    "name": "OutputWidth",
                    "default": "2992",
                    "type": "string"
                },
                {
                    "name": "OutputHeight",
                    "default": "2992",
                    "type": "string"
                },
                {
                    "name": "RTSPPort",
                    "default": "8554",
                    "type": "string"
                },
                {
                    "name": "Framerate",
                    "default": "15",
                    "type": "string"
                },
                {
                    "name": "CenterX",
                    "default": "0.5",
                    "type": "string"
                },
                {
                    "name": "CenterY",
                    "default": "0.5",
                    "type": "string"
                },
                {
                    "name": "PanAngle",
                    "default": "0",
                    "type": "string"
                },
                {
                    "name": "TiltAngle",
                    "default": "0",
                    "type": "string"
                },
                {
                    "name": "RectilinearFOV",
                    "default": "90",
                    "type": "string"
                },
                {
                    "name": "FocalLength",
                    "default": "1496",
                    "type": "string"
                },
                {
                    "name": "K1",
                    "default": "0.0",
                    "type": "string"
                },
                {
                    "name": "K2",
                    "default": "0.0",
                    "type": "string"
                },
                {
                    "name": "K3",
                    "default": "0.0",
                    "type": "string"
                },
                {
                    "name": "K4",
                    "default": "0.0",
                    "type": "string"
                },
                {
                    "name": "Scale",
                    "default": "0.5",
                    "type": "string"
                }
            ]
        }
    }
}
